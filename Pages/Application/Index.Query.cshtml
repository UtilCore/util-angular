@model ApplicationQuery

@*查询表单*@
<util-form layout="Inline" class="search__form" label-sm="8" label-xs="24" control-sm="14" control-xs="24" gutter="6">
    <util-row>
        <util-input for="Code"/>
        <util-input for="Name" />
        <util-column>
            <util-button id="btnQuery" type="Primary" icon="Search" on-click="query(btnQuery)">查询</util-button>
            <util-button id="btnRefresh" icon="Sync" on-click="refresh(btnRefresh)">刷新</util-button>
        </util-column>
    </util-row>
</util-form>

